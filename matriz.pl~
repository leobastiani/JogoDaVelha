matriz3x3Girar([
	[A00, A01, A02],
	[A10, A11, A12],
	[A20, A21, A22]
],

[
	[A00, A10, A20],
	[A01, A11, A21],
	[A02, A12, A22]
]).



matriz3x3Vazia([
	[0, 0, 0],
	[0, 0, 0],
	[0, 0, 0]
]).



matriz3x3Exemplo([
	[1, 2, 3],
	[4, 5, 6],
	[7, 8, 9]
]).


printMatriz(M) :-
	format("~w\n~w\n~w\n", M).


main(_) :-
	matriz3x3Vazia(Matriz),
	printMatriz(Matriz).

?- main(3).
